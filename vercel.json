{ 
  "version": 2,
  "builds": [
    {
      "src": "mai/wsgi.py",
      "use": "@github/python",
      "config":{"maxLambdaSize": "15mb", "runtime": "python3.12"}
    },
    {
      "src": "build_files.sh",
      "use": "@github/static-build",
      "config": {
        "distDir": "staticfiles_build"
      }
    }
  ],
  "routes": [
   
    {
      "src": "/static/(.*)",
      "dest": "/static/$1"
    }, {
      "src": "/(.*)",
      "dest": "mai/wsgi.py"
    },
  ]
}
